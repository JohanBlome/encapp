{
    "common": {
        "id": "dynamic_framerate",
        "description": "Dynamic Framerate Test"
    },
    "input": {
        "filepath": "foo.yuv",
        "resolution": "176x144",
        "pix-fmt": "nv12",
        "framerate": 30,
        "playout-frames": 300
    },
    "configure": {
        "codec": "OMX.google.h264.encoder",
        "bitrate": "300 kbps"
    },
    "runtime": [
        [
            60,
            "dynamic-framerate",
            15
        ],
        [
            120,
            "dynamic-framerate",
            5
        ],
        [
            180,
            "dynamic-framerate",
            20
        ],
        [
            240,
            "dynamic-framerate",
            30
        ]
    ]
}
