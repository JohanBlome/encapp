            },
            "input": {
                "filepath": "/sdcard/320x180_900fr_avc.mp4",
                "resolution": "320x180"
            },
            "configure": {
                "codec": "encoder.avc",
                "ts-schema": "android.generic.2"
            }
        }
    ],
    "parallel": [
