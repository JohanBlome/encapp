{
    "common": {
        "id": "extraconfig",
        "description": "Test suite using a serial test"
    },
    "serial": [
        {
            "common": {
                "description": "Configure TL and ltr count 2"
            },
            "input": {
                "filename": "/sdcard/320x180_900fr_nv12.yuv",
                "width": 320,
                "height": 180,
                "framerate": 30,
                "playout-frames": 300
            },
            "configure": {
                "codec": [
                    "string",
                    "encoder.avc"
                ],
                "width": [
                    "int",
                    320
                ],
                "height": [
                    "int",
                    180
                ],
                "ts-schema": [
                    "string",
                    "android.generic.2"
                ],
                "vendor.qti-ext-enc-caps-ltr.max-count": [
                    "int",
                    6
                ],
                "vendor.qti-ext-enc-ltr-count.num-ltr-frames": [
                    "int",
                    2
                ]
            }
        },
        {
            "common": {
                "description": "Temporal layers"
            },
            "input": {
                "filename": "/sdcard/320x180_900fr_avc.mp4",
                "width": 320,
                "height": 180
            },
            "configure": {
                "codec": [
                    "string",
                    "encoder.avc"
                ],
                "ts-schema": [
                    "string",
                    "android.generic.2"
                ]
            }
        },
        {
            "common": {
                "description": "Dynamic LTR"
            },
            "input": {
                "filename": "/sdcard/320x180_900fr_avc.mp4",
                "width": 320,
                "height": 180
            },
            "configure": {
                "codec": [
                    "string",
                    "encoder.avc"
                ],
                "ts-schema": [
                    "string",
                    "android.generic.2"
                ],
                "vendor.qti-ext-enc-caps-ltr.max-count": [
                    "int",
                    6
                ],
                "vendor.qti-ext-enc-ltr-count.num-ltr-frames": [
                    "int",
                    2
                ]
            },
            "runtime": [
                [
                    10,
                    "vendor.qti-ext-enc-ltr.mark-frame",
                    "int",
                    0
                ],
                [
                    20,
                    "vendor.qti-ext-enc-ltr.mark-frame",
                    "int",
                    1
                ],
                [
                    40,
                    "vendor.qti-ext-enc-ltr.use-frame",
                    "int",
                    0
                ],
                [
                    60,
                    "vendor.qti-ext-enc-ltr.use-frame",
                    "int",
                    1
                ]
            ]
        }
    ]
}
