test {
    common {
        id: "Bitrate"
        description: "Verify encoding bitrate"
    }
    input {
        filepath: "/tmp/akiyo_qcif.yuv"
        resolution: "176x144"
        framerate: 30
    }
    configure {
        codec: "OMX.google.h264.encoder"
        bitrate: "300 kbps"
        parameter {
            key: "ts-schema"
            type: stringType
            value: "android.generic.2"
        }
    }
    runtime {
        video_bitrate {
            framenum: 450
            bitrate: "500k"
        }
    }
}
