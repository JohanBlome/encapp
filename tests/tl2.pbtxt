test {
    common {
        id: "tl2"
        description: "Verify encoding bitrate on TL-2 (Temporal Layering)"
    }
    input {
        filepath: "/tmp/akiyo_qcif.yuv"
        resolution: "176x144"
        framerate: 30
        playout_frames: 900
    }
    configure {
        codec: "OMX.google.h264.encoder"
        bitrate: "100 kbps"
        parameter {
            key: "ts-schema"
            type: stringType
            value: "android.generic.2"
        }
    }
    runtime {
        video_bitrate {
            framenum: 400
            bitrate: "200k"
        }
    }
}
